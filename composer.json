{
    "name": "monkblog/theme-manager",
    "description": "A simple theme manager",
    "keywords": ["monk", "monkblog", "theme", "theme manager", "laravel", "manager"],
    "license": "MIT",
    "authors": [
      {
        "name": "Eric Dowell",
        "email": "eric@ericodwell.com"
      }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.0.*|5.1.*",
        "illuminate/filesystem": "5.0.*|5.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9.1",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "ThemeManager\\": "src/ThemeManager/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
      "preferred-install": "dist"
    }
}
